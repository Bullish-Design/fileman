{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lsd",
      "camelcase": "Lsd",
      "title": "Lsd",
      "scope": "source.lsd",
      "file-types": [
        "lsd"
      ],
      "injection-regex": "^lsd$",
      "class-name": "TreeSitterLsd"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Tree-Sitter grammar for parsing lsd cli output",
    "authors": [
      {
        "name": "Bullish-Design",
        "email": "bullishdesignllc@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-lsd"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}